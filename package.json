{
  "name": "apollo-engine",
  "version": "0.8.5",
  "description": "Sideload Apollo Engine in front of your graphql server",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run compile",
